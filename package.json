{
  "name": "async-didi",
  "version": "0.3.1",
  "description": "Async Dependency Injection for JavaScript",
  "main": "./dist/index.js",
  "source": "./lib/index.js",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "all": "run-s lint bundle test check-types integration-test",
    "bundle": "NODE_ENV=production rollup -c",
    "lint": "eslint .",
    "check-types": "tsc --pretty --noEmit",
    "test": "mocha test/*.spec.js",
    "integration-test": "(cd test/integration && mocha --import=tsx *.spec.{cjs,ts})",
    "prepare": "run-s bundle"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nikku/async-didi.git"
  },
  "keywords": [
    "di",
    "didi",
    "inversion of control",
    "dependency",
    "injection",
    "injector"
  ],
  "dependencies": {
    "didi": "^11.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.10.4",
    "@web/rollup-plugin-copy": "^0.5.1",
    "chai": "^6.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-bpmn-io": "^2.2.0",
    "mocha": "^11.7.5",
    "npm-run-all2": "^8.0.4",
    "rollup": "^4.53.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  },
  "engines": {
    "node": ">= 20.12"
  },
  "author": "Nico Rehwaldt <git_nikku@nixis.de>",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist"
  ]
}
