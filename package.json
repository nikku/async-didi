{
  "name": "async-didi",
  "version": "0.3.1",
  "description": "Async Dependency Injection for JavaScript",
  "main": "./dist/index.js",
  "source": "./lib/index.js",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "all": "run-s lint bundle test test:integration",
    "bundle": "NODE_ENV=production rollup -c",
    "lint": "eslint .",
    "test": "mocha test/*.spec.js",
    "test:integration": "mocha test/integration/*.spec.*js",
    "prepare": "run-s bundle",
    "prepublishOnly": "run-s test:integration"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nikku/async-didi.git"
  },
  "keywords": [
    "di",
    "didi",
    "inversion of control",
    "dependency",
    "injection",
    "injector"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "chai": "^6.2.1",
    "didi": "^6.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-bpmn-io": "^0.12.0",
    "mocha": "^11.7.5",
    "npm-run-all2": "^8.0.4",
    "rollup": "^2.59.0"
  },
  "engines": {
    "node": ">= 20.12"
  },
  "author": "Nico Rehwaldt <git_nikku@nixis.de>",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist"
  ]
}
